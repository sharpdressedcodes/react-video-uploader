{
    "name": "react-video-uploader",
    "version": "2.0.0",
    "description": "Video Uploader with React",
    "license": "UNLICENSED",
    "private": true,
    "contributors": [
        {
            "name": "Greg Kappatos",
            "email": "sharpdressedcodes@users.noreply.github.com"
        }
    ],
    "engines": {
        "node": ">=18.16.0",
        "npm": ">=9.5.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sharpdressedcodes/react-video-uploader.git"
    },
    "homepage": "https://github.com/sharpdressedcodes/react-video-uploader#readme",
    "browserslist": [
        ">0.2%",
        "last 2 versions",
        "Firefox ESR",
        "not dead",
        "node 18"
    ],
    "scripts": {
        "prepare": "node --stack-trace-limit=9999 --trace-warnings ./scripts/install-husky.js",
        "cli": "node --stack-trace-limit=9999 --trace-warnings ./scripts/cli.js",
        "clean": "rimraf ./build/server ./build/browser/fonts ./build/browser/images ./build/browser/*.js* ./build/browser/*.css && node -e \"fs.mkdirSync('./build/browser/data/uploads/videos', { recursive: true });\" && node -e \"fs.mkdirSync('./build/browser/data/uploads/contact', { recursive: true });\"",
        "build:browser": "cross-env APP_ENV=browser webpack --color --stats errors-warnings",
        "build:server": "cross-env APP_ENV=server webpack --color --stats errors-warnings",
        "build": "npm run clean && concurrently --kill-others-on-fail --names \"browser,server\" -c \"bgBlue.bold,bgCyan.bold\" \"cross-env NODE_ENV=production npm run build:browser -- --bail\" \"cross-env NODE_ENV=production npm run build:server -- --bail\"",
        "dev": "npm run clean && concurrently --kill-others-on-fail --names \"build,server\" -c \"bgBlue.bold,bgCyan.bold\" \"cross-env NODE_ENV=development FAST_REFRESH=true npm run build:server -- -w\" \"wait-on ./build/server/server.js && cross-env NODE_ENV=development FAST_REFRESH=true npm run start -- --watch --trace-warnings --stack-trace-limit=9999\"",
        "dev:no-watch": "npm run clean && concurrently --kill-others-on-fail --names \"browser,server\" -c \"bgBlue.bold,bgCyan.bold\" \"cross-env NODE_ENV=development npm run build:browser -- --bail\" \"cross-env NODE_ENV=development npm run build:server -- --bail\" && cross-env NODE_ENV=development npm run start -- --trace-warnings --stack-trace-limit=9999",
        "start": "node ./build/server/server.js",
        "lint:yml": "yamllint \"**/*.(yml|yaml)\" --ignore=\".idea\" --ignore=\".vscode\" --ignore=\".husky\" --ignore=\"build\" --ignore=\"node-cache\" --ignore=\"docker\" --ignore=\"hooks\" --ignore=\"node_modules\"",
        "lint:yml:staged": "yamllint --ignore=\".idea\" --ignore=\".vscode\" --ignore=\".husky\" --ignore=\"build\" --ignore=\"node-cache\" --ignore=\"docker\" --ignore=\"hooks\" --ignore=\"node_modules\"",
        "lint:md": "markdownlint-cli2 \"**/*.md\" \"#.idea\" \"#.vscode\" \"#.husky\" \"#build\" \"#node-cache\" \"#docker\" \"#hooks\" \"#node_modules\"",
        "lint:md:fix": "markdownlint-cli2-fix \"**/*.md\" \"#.idea\" \"#.vscode\" \"#.husky\" \"#build\" \"#node-cache\" \"#docker\" \"#hooks\" \"#node_modules\"",
        "lint:md:staged": "markdownlint-cli2-fix \"#.idea\" \"#.vscode\" \"#.husky\" \"#build\" \"#node-cache\" \"#docker\" \"#hooks\" \"#node_modules\"",
        "lint:ts": "tsc --noEmit",
        "lint:js": "eslint \"./**/*.{cjs,mjs,js,ts,jsx,tsx,html,vue}\"",
        "lint:js:staged": "eslint --fix --no-error-on-unmatched-pattern",
        "lint:js:fix": "npm run lint:js -- --fix",
        "lint:scss:staged": "stylelint --allow-empty-input --config ./stylelint.config.js --fix",
        "lint:scss": "stylelint --allow-empty-input --config ./stylelint.config.js \"./src/**/*.scss\"",
        "lint:scss:fix": "npm run lint:scss -- --fix",
        "lint": "concurrently --names \"eslint,tsc,stylelint,yamllint,markdownlint\" -c \"bgCyan.bold,bgYellow.bold,bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run lint:js\" \"npm run lint:ts\" \"npm run lint:scss\" \"npm run lint:yml\" \"npm run lint:md\"",
        "lint:fix": "concurrently --names \"eslint,tsc,stylelint,yamllint,markdownlint\" -c \"bgCyan.bold,bgYellow.bold,bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run lint:js:fix\" \"npm run lint:ts\" \"npm run lint:scss:fix\" \"npm run lint:yml\" \"npm run lint:md:fix\"",
        "test": "cross-env TEST=true jest --colors --passWithNoTests --testPathPattern=\"./src/.*.test.(j|t)sx?\"",
        "test:coverage": "npm run test -- --coverage",
        "test:ci": "cross-env CI=true npm run test",
        "test:ci:coverage": "cross-env CI=true npm run test:coverage",
        "test:watch": "npm run test -- --watch",
        "test:watch:coverage": "npm run test:coverage -- --watch",
        "test:e2e": "cross-env TEST=true playwright test",
        "test:e2e:ui": "npm run test:e2e -- --ui",
        "test:e2e:ci": "cross-env CI=true npm run test:e2e",
        "audit:ci": "npm audit --audit-level high --json"
    },
