{
    "name": "react-video-uploader",
    "version": "1.0.0",
    "description": "Video Uploader with React",
    "license": "UNLICENSED",
    "private": true,
    "contributors": [
        {
            "name": "Greg Kappatos",
            "email": "sharpdressedcodes@users.noreply.github.com"
        }
    ],
    "engines": {
        "node": ">=16.15.0",
        "npm": ">=8.5.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sharpdressedcodes/react-video-uploader.git"
    },
    "homepage": "https://github.com/sharpdressedcodes/react-video-uploader#readme",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "IE 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "IE 11"
        ]
    },
    "scripts": {
        "prepare": "node --stack-trace-limit=9999 ./scripts/install-husky.js",
        "clean": "rimraf ./build/bundle.* ./build/server.js && mkdirp ./build/data/uploads",
        "dev": "concurrently --names \"build,server\" -c \"bgBlue.bold,bgCyan.bold\" \"npm run build:dev:watch\" \"wait-on ./build/server.js && npm run start:dev\"",
        "start:dev": "cross-env NODE_ENV=development node --stack-trace-limit=9999 ./build/server.js",
        "start": "cross-env NODE_ENV=production node ./build/server.js",
        "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --bail --performance-hints warning --color --progress --stats \"errors-only\" --mode development",
        "build:dev:watch": "npm run clean && cross-env NODE_ENV=development webpack --watch --performance-hints warning --color --stats \"errors-only\" --mode development",
        "build": "npm run clean && cross-env NODE_ENV=production webpack --bail --stats \"errors-only\" --mode production",
        "lint:yml": "yamllint \"**/*.(yml|yaml)\" --ignore=\"node_modules\" --ignore=\"cypress-cache\"",
        "lint:yml:staged": "yamllint --ignore=\"node_modules\" --ignore=\"cypress-cache\"",
        "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\" \"#cypress-cache\"",
        "lint:md:fix": "markdownlint-cli2-fix \"**/*.md\" \"#node_modules\" \"#cypress-cache\"",
        "lint:md:staged": "markdownlint-cli2-fix \"#node_modules\"",
        "lint:js": "eslint ./src/**/*.js",
        "lint:js:staged": "eslint --fix",
        "lint:js:fix": "npm run lint:js -- --fix",
        "lint:scss:staged": "stylelint --allow-empty-input --config ./stylelint.config.js --fix",
        "lint:scss": "stylelint --allow-empty-input --config ./stylelint.config.js ./src/**/*.scss",
        "lint:scss:fix": "npm run lint:scss -- --fix",
        "lint": "concurrently --names \"yamllint,markdownlint,stylelint,eslint\" -c \"bgMagenta.bold,bgGreen.bold,bgBlue.bold,bgCyan.bold\" \"npm run lint:yml\" \"npm run lint:md\" \"npm run lint:scss\" \"npm run lint:js\"",
        "lint:fix": "concurrently --names \"yamllint,markdownlint,stylelint,eslint\" -c \"bgMagenta.bold,bgGreen.bold,bgBlue.bold,bgCyan.bold\" \"npm run lint:yml\" \"npm run lint:md:fix\" \"npm run lint:scss:fix\" \"npm run lint:js:fix\"",
        "test": "cross-env NODE_ENV=test jest --colors --passWithNoTests --testPathPattern=\"./src/.*.test.js\"",
        "test:coverage": "npm run test -- --coverage",
        "test:ci": "cross-env CI=true npm run test",
        "test:ci:coverage": "cross-env CI=true npm run test:coverage",
        "test:watch": "npm run test -- --watch",
        "test:watch:coverage": "npm run test:coverage -- --watch",
        "test:e2e": "node --stack-trace-limit=9999 ./scripts/run-e2e.js \"npx cypress run\"",
        "test:e2e:chrome": "echo \"Not Implemented yet!\"; return 0; node --stack-trace-limit=9999 ./scripts/run-e2e.js \"npx cypress run --browser chromium --headless\"",
        "test:e2e:chrome:headed": "echo \"Not Implemented yet!\"; return 0; node --stack-trace-limit=9999 ./scripts/run-e2e.js \"npx cypress run --browser chromium\"",
        "test:e2e:open": "node --stack-trace-limit=9999 ./scripts/run-e2e.js \"npx cypress open --browser --e2e --project .\"",
        "test:e2e:ci": "cross-env CI=true npm run test:e2e",
        "audit:ci": "npm audit --audit-level high --json"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "^7.18.9",
        "@material-ui/core": "^4.12.4",
        "@material-ui/icons": "^4.11.3",
        "@reduxjs/toolkit": "^1.8.5",
        "axios": "^0.27.2",
        "body-parser": "^1.20.0",
        "chalk": "^4.1.2",
        "classnames": "^2.3.1",
        "cookie-parser": "^1.4.6",
        "core-js": "^3.25.0",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "express": "^4.18.1",
        "fluent-ffmpeg": "^2.1.2",
        "keymirror": "^0.1.1",
        "lodash.get": "^4.4.2",
        "lodash.isequal": "^4.5.0",
        "mkdirp": "^1.0.4",
        "multer": "^1.4.5-lts.1",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "react-toastify": "^9.0.8",
        "regenerator-runtime": "^0.13.9",
        "serialize-javascript": "^6.0.0",
        "uuid": "^8.3.2",
        "ws": "^8.8.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/core": "^7.18.13",
        "@babel/eslint-parser": "^7.18.9",
        "@babel/eslint-plugin": "^7.18.10",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.18.10",
        "@babel/plugin-proposal-destructuring-private": "^7.18.9",
        "@babel/plugin-proposal-export-default-from": "^7.18.10",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@babel/register": "^7.18.9",
        "@svgr/webpack": "^6.3.1",
        "autoprefixer": "^10.4.8",
        "babel-jest": "^29.0.2",
        "babel-loader": "^8.2.5",
        "concurrently": "^7.3.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "cypress": "^10.7.0",
        "eslint": "8.22.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-destructuring": "^2.2.1",
        "eslint-plugin-react": "^7.31.4",
        "file-loader": "^6.2.0",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.0.3",
        "husky": "^8.0.1",
        "html-webpack-plugin": "^5.5.0",
        "lint-staged": "^13.0.3",
        "markdownlint-cli2": "^0.5.1",
        "micromatch": "^4.0.5",
        "mini-css-extract-plugin": "^2.6.1",
        "normalize.css": "^8.0.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^10.0.0",
        "postcss-loader": "^7.0.1",
        "resolve-url-loader": "^5.0.0",
        "rimraf": "^3.0.2",
        "sass": "^1.54.8",
        "sass-loader": "^13.0.2",
        "stylelint": "^14.10.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "terser-webpack-plugin": "^5.3.6",
        "url-loader": "^4.1.1",
        "wait-on": "^6.0.1",
        "webpack": "^5.76.0",
        "webpack-cli": "^4.10.0",
        "webpack-node-externals": "^3.0.0",
        "yaml-lint": "^1.7.0"
    }
}
