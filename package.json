{
    "name": "react-video-uploader",
    "version": "2.0.0",
    "description": "Video Uploader with React",
    "license": "UNLICENSED",
    "private": true,
    "contributors": [
        {
            "name": "Greg Kappatos",
            "email": "sharpdressedcodes@users.noreply.github.com"
        }
    ],
    "engines": {
        "node": ">=18.16.0",
        "npm": ">=9.5.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sharpdressedcodes/react-video-uploader.git"
    },
    "homepage": "https://github.com/sharpdressedcodes/react-video-uploader#readme",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "IE 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "IE 11"
        ]
    },
    "scripts": {
        "prepare": "node --stack-trace-limit=9999 --trace-warnings ./scripts/install-husky.js",
        "clean": "rimraf ./build/*.js* ./build/*.css ./src/common/index.cjs ./src/config/index.cjs ./src/routes/paths.cjs && node -e \"fs.mkdirSync('./build/data/uploads', { recursive: true });\"",
        "build:common": "webpack --config webpack-common.config.js --performance-hints warning --color --stats errors-only",
        "build:browser": "cross-env APP_ENV=browser webpack --color --stats errors-warnings --config webpack.config.js",
        "build:server": "cross-env APP_ENV=server webpack --color --stats errors-warnings --config webpack.config.js",
        "build": "npm run clean && cross-env NODE_ENV=production npm run build:common -- --bail && cross-env NODE_ENV=production npm run build:browser -- --bail && cross-env NODE_ENV=production npm run build:server -- --bail && rimraf ./build/*.LICENSE.txt",
        "dev": "npm run clean && concurrently --kill-others-on-fail --names \"common,build,server\" -c \"bgMagenta.bold,bgBlue.bold,bgCyan.bold\" \"cross-env NODE_ENV=development FAST_REFRESH=true npm run build:common -- -w\" \"wait-on ./src/config/index.cjs && cross-env NODE_ENV=development FAST_REFRESH=true npm run build:server -- -w\" \"wait-on ./build/server-entry.js && cross-env NODE_ENV=development FAST_REFRESH=true npm run start -- --watch --trace-warnings --stack-trace-limit=9999\"",
        "dev:no-watch": "npm run clean && cross-env NODE_ENV=development npm run build:common -- --progress --bail && cross-env NODE_ENV=development npm run build:browser -- --progress --bail && cross-env NODE_ENV=development npm run build:server -- --progress --bail && cross-env NODE_ENV=development npm run start -- --trace-warnings --stack-trace-limit=9999",
        "start": "node ./src/server/index.js",
        "lint:yml": "yamllint \"**/*.(yml|yaml)\" --ignore=\".idea\" --ignore=\".vscode\" --ignore=\".husky\" --ignore=\"build\" --ignore=\"cypress-cache\" --ignore=\"docker\" --ignore=\"hooks\" --ignore=\"node_modules\"",
        "lint:yml:staged": "yamllint --ignore=\".idea\" --ignore=\".vscode\" --ignore=\".husky\" --ignore=\"build\" --ignore=\"cypress-cache\" --ignore=\"docker\" --ignore=\"hooks\" --ignore=\"node_modules\"",
        "lint:md": "markdownlint-cli2 \"**/*.md\" \"#.idea\" \"#.vscode\" \"#.husky\" \"#build\" \"#cypress-cache\" \"#docker\" \"#hooks\" \"#node_modules\"",
        "lint:md:fix": "markdownlint-cli2-fix \"**/*.md\" \"#.idea\" \"#.vscode\" \"#.husky\" \"#build\" \"#cypress-cache\" \"#docker\" \"#hooks\" \"#node_modules\"",
        "lint:md:staged": "markdownlint-cli2-fix \"#.idea\" \"#.vscode\" \"#.husky\" \"#build\" \"#cypress-cache\" \"#docker\" \"#hooks\" \"#node_modules\"",
        "lint:js": "eslint --ignore-pattern \"node_modules/*\" \"./**/*.js\"",
        "lint:js:staged": "eslint --ignore-pattern \"node_modules/*\" \"./**/*.js\" --fix",
        "lint:js:fix": "npm run lint:js -- --fix",
        "lint:scss:staged": "stylelint --allow-empty-input --config ./stylelint.config.js --fix",
        "lint:scss": "stylelint --allow-empty-input --config ./stylelint.config.js \"./src/**/*.scss\"",
        "lint:scss:fix": "npm run lint:scss -- --fix",
        "lint": "concurrently --names \"yamllint,markdownlint,stylelint,eslint\" -c \"bgMagenta.bold,bgGreen.bold,bgBlue.bold,bgCyan.bold\" \"npm run lint:yml\" \"npm run lint:md\" \"npm run lint:scss\" \"npm run lint:js\"",
        "lint:fix": "concurrently --names \"yamllint,markdownlint,stylelint,eslint\" -c \"bgMagenta.bold,bgGreen.bold,bgBlue.bold,bgCyan.bold\" \"npm run lint:yml\" \"npm run lint:md:fix\" \"npm run lint:scss:fix\" \"npm run lint:js:fix\"",
        "test": "rimraf ./cypress-cache/Cypress ./cypress-cache/mesa_shader_cache && cross-env NODE_ENV=test jest --colors --passWithNoTests --testPathPattern=\"./src/.*.test.js\"",
        "test:coverage": "npm run test -- --coverage",
        "test:ci": "cross-env CI=true npm run test",
        "test:ci:coverage": "cross-env CI=true npm run test:coverage",
        "test:watch": "npm run test -- --watch",
        "test:watch:coverage": "npm run test:coverage -- --watch",
        "test:e2e": "node --stack-trace-limit=9999 ./scripts/run-e2e.js \"npx cypress run\"",
        "test:e2e:chrome": "echo \"Not Implemented yet!\"; return 0; node --stack-trace-limit=9999 ./scripts/run-e2e.js \"npx cypress run --browser chromium --headless\"",
        "test:e2e:chrome:headed": "echo \"Not Implemented yet!\"; return 0; node --stack-trace-limit=9999 ./scripts/run-e2e.js \"npx cypress run --browser chromium\"",
        "test:e2e:open": "node --stack-trace-limit=9999 ./scripts/run-e2e.js \"npx cypress open --browser --e2e --project .\"",
        "test:e2e:ci": "cross-env CI=true npm run test:e2e",
        "audit:ci": "npm audit --audit-level high --json"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "7.21.5",
        "@material-ui/core": "4.12.4",
        "@material-ui/icons": "4.11.3",
        "@reduxjs/toolkit": "1.9.5",
        "axios": "1.4.0",
        "body-parser": "1.20.2",
        "chalk": "4.1.2",
        "classnames": "2.3.2",
        "cookie-parser": "1.4.6",
        "core-js": "3.30.2",
        "cors": "2.8.5",
        "csurf": "1.11.0",
        "express": "4.18.2",
        "fluent-ffmpeg": "2.1.2",
        "keymirror": "0.1.1",
        "lodash.get": "4.4.2",
        "lodash.isequal": "4.5.0",
        "multer": "1.4.5-lts.1",
        "prop-types": "15.8.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-redux": "8.0.5",
        "react-refresh": "0.14.0",
        "react-router-dom": "6.11.1",
        "react-toastify": "9.1.2",
        "regenerator-runtime": "0.13.11",
        "serialize-javascript": "6.0.1",
        "uuid": "9.0.0",
        "ws": "8.13.0"
    },
    "devDependencies": {
        "@babel/cli": "7.21.5",
        "@babel/core": "7.21.8",
        "@babel/eslint-parser": "7.21.8",
        "@babel/eslint-plugin": "7.19.1",
        "@babel/plugin-proposal-class-properties": "7.18.6",
        "@babel/plugin-proposal-decorators": "7.21.0",
        "@babel/plugin-proposal-destructuring-private": "7.21.0",
        "@babel/plugin-proposal-export-default-from": "7.18.10",
        "@babel/plugin-transform-runtime": "7.21.4",
        "@babel/preset-env": "7.21.5",
        "@babel/preset-react": "7.18.6",
        "@babel/register": "7.21.0",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
        "@svgr/webpack": "8.0.1",
        "autoprefixer": "10.4.14",
        "babel-jest": "29.5.0",
        "babel-loader": "9.1.2",
        "concurrently": "8.0.1",
        "cross-env": "7.0.3",
        "css-loader": "6.7.3",
        "cypress": "10.8.0",
        "eslint": "8.40.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-plugin-babel": "5.3.1",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-destructuring": "2.2.1",
        "eslint-plugin-react": "7.32.2",
        "file-loader": "6.2.0",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "husky": "8.0.3",
        "lint-staged": "13.2.2",
        "markdownlint-cli2": "0.7.1",
        "micromatch": "4.0.5",
        "mini-css-extract-plugin": "2.7.5",
        "normalize.css": "8.0.1",
        "postcss-cli": "10.1.0",
        "postcss-loader": "7.3.0",
        "resolve-url-loader": "5.0.0",
        "rimraf": "5.0.0",
        "sass": "1.62.1",
        "sass-loader": "13.2.2",
        "stylelint": "15.6.1",
        "stylelint-config-standard-scss": "9.0.0",
        "terser-webpack-plugin": "5.3.8",
        "url-loader": "4.1.1",
        "wait-on": "7.0.1",
        "webpack": "5.82.0",
        "webpack-cli": "5.1.1",
        "webpack-dev-middleware": "6.1.0",
        "webpack-hot-middleware": "2.25.3",
        "webpack-node-externals": "3.0.0",
        "yaml-lint": "1.7.0"
    }
}
